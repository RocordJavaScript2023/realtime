{
  "name": "realtime",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node ./src/server.ts",
    "dev:debug": "ts-node-dev --transpile-only --respawn --inspect=4321 --project tsconfig.json ./src/server.ts",
    "build": "next  build",
    "start": "NODE_ENV=production && next start --turbo",
    "lint": "next lint"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^4.15.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.38.0",
    "eslint-config-next": "^13.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^8.8.0",
    "next": "^13.4.6",
    "next-auth": "^4.22.1",
    "nodemon": "^2.0.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-lottie-player": "^1.5.4",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "prisma": "^4.15.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
